{
    "name": "jan-herman/kirby-cookieconsent",
    "description": "CookieConsent.js integration for Kirby CMS.",
    "version": "1.0.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-cookieconsent",
    "authors": [
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.2"
    },
    "extra": {
        "installer-name": "cookieconsent"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\CookieConsent\\": "lib/"
        }
    }
}